{
  "name": "basic-app",
  "description": "Demo Application",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "./index.js",
  "workspaces": {
    "nohoist": [
      "*",
      "*/**"
    ]
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "npm run env -- prod && react-native start",
    "start:dev": "npm run env -- dev && react-native start --reset-cache",
    "env": "node ./env/set-env.js",
    "build": "tsc -p ./tsconfig.json",
    "build:web": "expo build:web",
    "clean": "rimraf ./dist ./web-build",
    "lint": "tslint -c ./tslint.json ./src/**/*.{ts,tsx}",
    "changelog": "conventional-changelog -i ./CHANGELOG.md -s",
    "postinstall": "jetify"
  },
  "dependencies": {
    "@react-native-community/async-storage": "~1.11.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.6.1",
    "@react-navigation/drawer": "^5.6.1",
    "@react-navigation/material-top-tabs": "^5.2.16",
    "@react-navigation/native": "^5.6.1",
    "@react-navigation/stack": "^5.6.2",
    "date-fns": "^1.30.1",
    "expo": "~38.0.8",
    "expo-constants": "~9.1.1",
    "expo-web-browser": "~8.3.1",
    "moment": "^2.24.0",
    "react": "~16.11.0",
    "react-dom": "~16.11.0",
    "react-native": "~0.62.2", 
    "react-native-device-info": "^5.5.1",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-paper": "^4.0.1",
    "react-native-reanimated": "~1.9.0",
    "react-native-safari-view": "^2.1.0",
    "react-native-safe-area-context": "~3.0.7",
    "react-native-screens": "~2.9.0",
    "react-native-svg": "12.1.0",
    "react-native-tab-view": "^2.13.0",
    "react-native-vector-icons": "^7.0.0",
    "react-native-web": "~0.11.7",
    "typeface-roboto": "^0.0.54"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/runtime": "^7.8.6",
    "@expo/webpack-config": "^0.12.24",
    "@types/react": "~16.9.23",
    "@types/react-native": "~0.62.13",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "~8.2.1",
    "conventional-changelog-cli": "^2.0.21",
    "expo-cli": "^3.11.9",
    "husky": "^1.1.2",
    "jetifier": "^1.6.5",
    "rimraf": "^2.6.2",
    "tslint": "^6.0.0",
    "typescript": "~3.9.5"
  }
